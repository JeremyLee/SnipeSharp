root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true
end_of_line = lf

[*.md]
trim_trailing_whitespace = false

[*.sln]
end_of_line = crlf
indent_style = tab
tab_width = 4

[*.ps1xml]
indent_size = 2

[*.csproj]
indent_size = 2

[*.nuspec]
indent_size = 2
